{
  "name": "figma-clean-document",
  "version": "2.2.0",
  "description": "A Figma plugin to automagically organize and clean up your Figma document",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/figma-plugins.git",
    "directory": "packages/figma-clean-document"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "build-figma-plugin --typecheck --minify",
    "clean": "rimraf '*.log' build 'src/**/*.css.d.ts'",
    "fix": "concurrently npm:fix:css npm:fix:js npm:fix:json",
    "fix:css": "stylelint --fix 'src/**/*.css'",
    "fix:js": "eslint --fix 'src/**/*.{ts,tsx}'",
    "fix:json": "prettier --log-level error --write '*.json'",
    "lint": "concurrently npm:lint:css npm:lint:js",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint 'src/**/*.{ts,tsx}'",
    "prewatch": "npm run clean",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "dependencies": {
    "@create-figma-plugin/ui": "4.0.1",
    "@create-figma-plugin/utilities": "4.0.1",
    "figma-sort-layers": "*",
    "preact": "10.26.9"
  },
  "figma-plugin": {
    "name": "Clean Document",
    "id": "767379019764649932",
    "editorType": [
      "figma"
    ],
    "menu": [
      {
        "name": "Clean Layers",
        "main": "src/commands/clean-nodes/main.ts",
        "ui": "src/commands/clean-nodes/ui.ts"
      },
      "-",
      {
        "name": "Delete Hidden Layers",
        "main": "src/commands/delete-hidden-nodes/main.ts"
      },
      {
        "name": "Ungroup Single-Layer Groups",
        "main": "src/commands/ungroup-single-node-groups/main.ts"
      },
      {
        "name": "Make Pixel-Perfect",
        "main": "src/commands/make-pixel-perfect/main.ts"
      },
      {
        "name": "Smart Rename Layers",
        "main": "src/commands/smart-rename-nodes/main.ts",
        "ui": "src/commands/smart-rename-nodes/ui.ts"
      },
      {
        "name": "Smart Sort Layers",
        "main": "src/commands/smart-sort-nodes/main.ts"
      },
      "-",
      {
        "name": "Position Canvas at (0,0)",
        "main": "src/commands/position-canvas-at-zero-zero/main.ts"
      },
      "-",
      {
        "name": "Sort Pages by Name",
        "main": "src/commands/sort-pages-by-name/main.ts"
      },
      "-",
      {
        "name": "Reset Plugin",
        "main": "src/commands/reset-plugin/main.ts"
      }
    ],
    "relaunchButtons": {
      "cleanLayers": {
        "name": "Clean Layers",
        "main": "src/commands/clean-nodes/main.ts",
        "ui": "src/commands/clean-nodes/ui.ts"
      }
    },
    "documentAccess": "dynamic-page",
    "networkAccess": {
      "allowedDomains": [
        "none"
      ]
    }
  }
}
